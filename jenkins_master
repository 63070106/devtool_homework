pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('read'){
            steps{
                script{
                    def data = readFile(file: 'data.json')
                    // build job: 'slave', parameters: [string(name: 'NAME', value: data.data.name), string(name: 'Ages', value: data.data.age)]
                    println(data.name)
                }
            }
        }
        // stage('Tigger the Slave Jobs') {
